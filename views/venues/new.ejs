<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/venues/new.css" />
<%- include('../partials/nav') %>

<main>
	<h1><%= title %></h1>
  <p>Please first ensure that the Venue is not in the dropdown
    <select>
      <% venues.forEach(function(venue) { %>
        <option><%= venue.name %></option>
      <% }) %>
    </select>
  </p>
  <form id="add-venue-form" action="/venues" method="POST">
    <label for="name-input">Name:</label>
    <input type="text" name="name" id="name-input">

    <label for="city-input">City:</label>
    <input type="text" name="city" id="city-input">

    <label for="state-input">State:</label>
    <input type="text" name="state" id="name-input">

    <button type="submit">Add Venue</button>
  </form>
</main>

<%- include('../partials/footer') %>